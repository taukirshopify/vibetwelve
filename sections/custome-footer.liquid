            <div class="row gy-2 align-items-center justify-content-between">
                <div class="col-md-auto wow fadeInUp" data-wow-delay="0.4s">
                    <div class="footer-menu">
                        <ul>
                            {% assign footer_menu_4 = section.settings.sub4 %}
                            {% for link in linklists[footer_menu_4].links %}
                            <li><a href="{{ link.url }}">{{link.title}}</a></li>
                
                            {% endfor %}
                        </ul>
                    </div>
                </div>
                <div class="col-md-auto wow fadeInUp" data-wow-delay="0.5s">
                    <p class="copyright">{{ section.settings.p }}</p>
                </div>
            </div>
        </div>

    </div>
</footer>


{% schema %}
    {
        "name": "Footer",
        "class": "f",
        "settings": [
            {
                "type": "image_picker",
                "id": "img",
                "label": "Select your Image"
            },
        {
            "type": "text",
            "id": "contact",
            "label": "Contact title",
            "default": "Contact us",
            "info": "Contact us",
            "placeholder": "Contact us"
        },
        {
            "type": "text",
            "id": "location",
            "label": "Put your Location",
            "default": "135 Westwood Ave. Westwood NJ 07675",
            "info": "135 Westwood Ave. Westwood NJ 07675",
            "placeholder": "135 Westwood Ave. Westwood NJ 07675"
        },
        {
            "type": "text",
            "id": "mail",
            "label": "Put your Mail",
            "default": "info@vibetwelve.com ",
            "info": "info@vibetwelve.com ",
            "placeholder": "info@vibetwelve.com "
        },
        {
            "type": "text",
            "id": "tell",
            "label": "Put your Email address",
            "default": "(201) 497-3818",
            "info": "(201) 497-3818",
            "placeholder": "(201) 497-3818"
        },
         {
            "type": "link_list",
            "id": "sub1",
            "label": "Footer First menu"
         },

         {
            "type": "link_list",
            "id": "sub4",
            "label": "Footer Second menu"
         },
         {
            "type": "text",
            "id": "p",
            "label": "Shop Copy Right text",
            "default": "© Vibe Tweleve 2023, All Rights Reserved",
      
            "placeholder": "© Vibe Tweleve 2023, All Rights Reserved"
         }
        ]
    }
{% endschema %}
