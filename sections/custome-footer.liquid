                        <h4 class="widget_title">{{ section.settings.contact }}</h4>
                        <div class="contact-widget">
                            <p><a href="#">{{ section.settings.location }}</a></p>
                            <p><a href="mailto:{{ section.settings.mail }}">{{ section.settings.mail }}</a></p>
